{
  "extends": [
    "config:base",
    ":disableDependencyDashboard",
    "helpers:pinGitHubActionDigests"
  ],
  "rangeStrategy": "replace",
  "semanticCommitType": "build",
  "packageRules": [
    {
      "depTypeList": ["dependencies"],
      "semanticCommitType": "fix"
    },
    {
      "depTypeList": ["action"],
      "semanticCommitType": "ci",
      "semanticCommitScope": "action"
    }
  ]
}
